package central;

import java.io.IOException;

public class main {

	public static void main(String[] args) {
		System.out.println("ETAPE1: Sockets:--------------------------------------");

		try {
			DataTransferServer ds = new DataTransferServer();
			ds.setupConnection();
			ds.connect();
		} catch (IOException | ClassNotFoundException e) {
			e.printStackTrace();
		}

	}

}