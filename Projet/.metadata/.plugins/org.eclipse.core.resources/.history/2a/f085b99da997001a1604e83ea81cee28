
package centralApplication;

import java.time.LocalDate;

public class TimeKeeper {
	private LocalDate current_Date;

	

	/**
	 * @return the current_Date
	 */
	public LocalDate getCurrent_Date() {
		return current_Date;
	}

	/**
	 * @param current_Date the current_Date to set
	 */
	public void setCurrent_Date(LocalDate current_Date) {
		this.current_Date = current_Date;
	}
	
	/**
	 * @brief Update current date
	 */
	public void updateCurrentDate() {
		this.setCurrent_Date(java.time.LocalDate.now());
	}

}
